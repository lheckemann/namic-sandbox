/* 
 * here is where system comupted values get stored these values should only
 * change when the target compile platform changes 
 */

/* what byte order */
#cmakedefine CMAKE_WORDS_BIGENDIAN
#ifdef CMAKE_WORDS_BIGENDIAN
  #define MRML_WORDS_BIGENDIAN 
#endif

/* what threading system are we using */
#cmakedefine CMAKE_USE_PTHREADS
#ifdef CMAKE_USE_PTHREADS
#define MRML_USE_PTHREADS
#endif

#cmakedefine CMAKE_USE_SPROC
#ifdef CMAKE_USE_SPROC
#define MRML_USE_SPROC
#endif

#cmakedefine CMAKE_HP_PTHREADS
#ifdef CMAKE_HP_PTHREADS
#define MRML_HP_PTHREADS
#endif

#cmakedefine CMAKE_USE_WIN32_THREADS
#ifdef CMAKE_USE_WIN32_THREADS
#define MRML_USE_WIN32_THREADS
#endif

#cmakedefine MRML_BUILD_SHARED_LIBS
#ifdef MRML_BUILD_SHARED_LIBS
#define MRMLDLL
#else
#define MRMLSTATIC
#endif

#cmakedefine CMAKE_NO_STD_NAMESPACE
#cmakedefine CMAKE_NO_ANSI_STREAM_HEADERS
#cmakedefine CMAKE_NO_ANSI_STRING_STREAM
#cmakedefine CMAKE_NO_ANSI_FOR_SCOPE
#cmakedefine MRML_CPP_FUNCTION

#define MRML_VERSION_MAJOR @MRML_VERSION_MAJOR@
#define MRML_VERSION_MINOR @MRML_VERSION_MINOR@
#define MRML_VERSION_PATCH @MRML_VERSION_PATCH@
#define MRML_VERSION_STRING "@MRML_VERSION_STRING@"
